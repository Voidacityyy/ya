	keyframe[19]=function()
		resetWelds(3,{})
		RootJoint.C0=RootJoint.C0 * CFrame.new(0,0,-.9) * CFrame.fromEulerAnglesXYZ(math.rad(-2),-math.rad(25),-math.rad(75))

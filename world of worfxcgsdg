						Tool.Status.AnimFrame.Value=RequestNewFrame(Tool.Status.AnimFrame.Value)
						applyDamage(v,16,-CameraPlane*40+Vector3.new(0,12,0),1,true,true,0,addonCullMovers,SoundTable["Hit_Damage_Heavycut"],13,25)
						applyHitEffect(v,0)
						DidHit=true
						CanInterrupt(true)

				applyDamage(v,30,Vector3.new(0,40,0),1,true,true,0,nil,SoundTable["Hit_Damage_Mediumcut"],0,40)
				DidHit=true
			end
		end
	end
	if Hitstop<=0 then
		if rawframes==10 then
			Hitstop=32
		end
		rawframes=rawframes+1
	end
	if ex<=7 then
		if oldp==nil then

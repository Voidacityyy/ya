--------------------------------------TURN ON AND OFF 
local edit = function(name,mat,col)
name.Material = mat
name.BrickColor = BrickColor.new(col)
end

local tog = function()
edit(TR1,"Neon","Lime green")
edit(TR2,"Neon","Lime green")
edit(TR3,"Neon","Lime green")
edit(TR4,"Neon","Lime green")
edit(TR5,"Neon","Lime green")
edit(TR6,"Neon","Lime green")
edit(TR7,"Neon","Lime green")
--
edit(TN1,"Neon","Lime green")
edit(TN2,"Neon","Lime green")
edit(TN3,"Neon","Lime green")

end
local tofg = function()
edit(TR1,"SmoothPlastic","Lime green")
edit(TR2,"SmoothPlastic","Lime green")
edit(TR3,"SmoothPlastic","Lime green")
edit(TR4,"SmoothPlastic","Lime green")
edit(TR5,"SmoothPlastic","Lime green")
edit(TR6,"SmoothPlastic","Lime green")
edit(TR7,"SmoothPlastic","Lime green")
--
edit(TN1,"SmoothPlastic","Lime green")
edit(TN2,"SmoothPlastic","Lime green")
edit(TN3,"SmoothPlastic","Lime green")
end

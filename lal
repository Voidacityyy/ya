		LW.C0 = clerp(LW.C0, CFrame.new(0, 0.5, -1) * angles(math.rad(0), math.rad(-160), math.rad(-90)), .15)
		RH.C0 = clerp(RH.C0, cn(1, -1, 0) * RHCF * angles(math.rad(-5), math.rad(-30), math.rad(0)), .15)

function Wave()
	attack = true
	Character.Humanoid.WalkSpeed = 5
	for i = 0, 1, 0.1 do
		swait()
		RootJoint.C0 = clerp(RootJoint.C0, RootCF * cn(0, 0, 0) * angles(math.rad(-20), math.rad(0), math.rad(0)), .3)
		Torso.Neck.C0 = clerp(Torso.Neck.C0, NeckCF * angles(math.rad(10), math.rad(0), math.rad(0)), .3)
		RW.C0 = clerp(RW.C0, CFrame.new(1.2, 1, .7) * angles(math.rad(180), math.rad(0), math.rad(-50)), .3)
		LW.C0 = clerp(LW.C0, CFrame.new(-1.2, 1, .7) * angles(math.rad(180), math.rad(0), math.rad(50)), .3)
		RH.C0 = clerp(RH.C0, cn(1, -1, 0) * RHCF * angles(math.rad(-3), math.rad(0), math.rad(-20)), .2)
		LH.C0 = clerp(LH.C0, cn(-1, -1, 0) * LHCF * angles(math.rad(-3), math.rad(0), math.rad(20)), .2)
		FakeHandleWeld.C0 = clerp(FakeHandleWeld.C0, cn(0, 0, 0) * angles(math.rad(-10), math.rad(10), math.rad(50)), .3)
		PHC1weld.C0 = clerp(PHC1weld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(0), math.rad(0)), .4)
		PHC2weld.C0 = clerp(PHC2weld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(0), math.rad(0)), .4)
	end
	local HitCon = HitboxA.Touched:connect(function(hit)
		Damage(hit.Parent, math.random(20, 30), 0.2, BrickColor.new("Really black"), BrickColor.new("Royal purple"), "rbxassetid://199149137" , 1)
	end)
	Effect1(RootPart.CFrame * CFrame.Angles(0, 3.2, 1.5), CFrame.new(0, 0, 1.5), 10, 150, 10, 1, 0.85, 0.01)
	SphereEffect(BrickColor.new("Royal purple"), RootPart.CFrame, 4, 4, 4, 4, 4, 4, .05)
	for i = 1, 2 do
		CreateSound("http://roblox.com/asset/?id=320557563", Torso, 1, 0.7)
		CreateSound("http://roblox.com/asset/?id=228343249", Torso, 1, 1) 
	end
	for i = 0, 1, 0.07 do
		swait()
		BreakEffect(BrickColor.new("Royal purple"), Barrel.CFrame, 1, 2, 3)
		BreakEffect(BrickColor.new("Royal purple"), Barrel.CFrame, 1, 2, 3)
		RootJoint.C0 = clerp(RootJoint.C0, RootCF * cn(0, 0, 0) * angles(math.rad(20), math.rad(0), math.rad(0)), .4)
		Torso.Neck.C0 = clerp(Torso.Neck.C0, NeckCF * angles(math.rad(-10), math.rad(0), math.rad(0)), .4)
		RW.C0 = clerp(RW.C0, CFrame.new(1, .5, -.7) * angles(math.rad(40), math.rad(0), math.rad(-50)), .4)
		LW.C0 = clerp(LW.C0, CFrame.new(-1, .5, -.7) * angles(math.rad(40), math.rad(0), math.rad(50)), .4)
		RH.C0 = clerp(RH.C0, cn(1, -1, 0) * RHCF * angles(math.rad(-3), math.rad(0), math.rad(20)), .3)
		LH.C0 = clerp(LH.C0, cn(-1, -1, 0) * LHCF * angles(math.rad(-3), math.rad(0), math.rad(-20)), .3)
		FakeHandleWeld.C0 = clerp(FakeHandleWeld.C0, cn(0, 0, 0) * angles(math.rad(20), math.rad(-20), math.rad(50)), .4)
		PHC1weld.C0 = clerp(PHC1weld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(0), math.rad(0)), .6)
		PHC2weld.C0 = clerp(PHC2weld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(0), math.rad(0)), .6)
	end
	HitCon:disconnect()
	attack = false
	Character.Humanoid.WalkSpeed = 10
end

local charge = false

function Decapitate()
	attack = true
	Character.Humanoid.WalkSpeed = 5
	CreateSound("http://roblox.com/asset/?id=360125304", Torso, 1, 1)
	while true do
		swait()
		if charge == true then
			RootJoint.C0 = clerp(RootJoint.C0, RootCF * cn(0, 0, 0) * angles(math.rad(0), math.rad(0), math.rad(-50)), .4)
			Torso.Neck.C0 = clerp(Torso.Neck.C0, NeckCF * angles(math.rad(0), math.rad(0), math.rad(50)), .4)
			RW.C0 = clerp(RW.C0, CFrame.new(1.3, 0.5, -.3) * angles(math.rad(0), math.rad(70), math.rad(80)), .4)
			LW.C0 = clerp(LW.C0, CFrame.new(-1, 0.5, -1) * angles(math.rad(0), math.rad(-130), math.rad(-90)), .4)
			RH.C0 = clerp(RH.C0, cn(1, -1, 0) * RHCF * angles(math.rad(-5), math.rad(30), math.rad(0)), .4)
			LH.C0 = clerp(LH.C0, cn(-1, -1, 0) * LHCF * angles(math.rad(-5), math.rad(30), math.rad(0)), .4)
			FakeHandleWeld.C0 = clerp(FakeHandleWeld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(-10), math.rad(0)), .3)
			PH1Weld.C0 = clerp(PH1Weld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(60), math.rad(0)), .1)
			PH2Weld.C0 = clerp(PH2Weld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(60), math.rad(0)), .1)
		elseif charge == false then
			break
		end
	end
	CreateSound("http://roblox.com/asset/?id=360125313", Torso, 1, 1)
	con1 = HitboxA.Touched:connect(function(hit) 
		CreateSound("http://roblox.com/asset/?id=206082327", hit.Parent.Head, 1, 1)
		Damage(hit.Parent, math.random(30, 50), 0.2, BrickColor.new("Really black"), BrickColor.new("Royal purple"), "rbxassetid://199149137" , 1)
		for i = 1, 10 do
			BreakEffect(BrickColor.new("Really red"), hit.Parent:WaitForChild("Head").CFrame, 0.5, math.random(5, 20), 0.5)
		end
		con1:disconnect()
	end) 
	con2 = HitboxB.Touched:connect(function(hit) 
		CreateSound("http://roblox.com/asset/?id=206082273", hit.Parent.Head, 1, 1)
		Damage(hit.Parent, math.random(30, 50), 0.2, BrickColor.new("Really black"), BrickColor.new("Royal purple"), "rbxassetid://199149137" , 1)
		for i = 1, 10 do
			BreakEffect(BrickColor.new("Really red"), hit.Parent:WaitForChild("Head").CFrame, 0.5, math.random(5, 20), 0.5)
		end
		con2:disconnect()
	end) 
	for i = 0, 1, 0.03 do
		swait()
		RootJoint.C0 = clerp(RootJoint.C0, RootCF * cn(0, 0, 0) * angles(math.rad(0), math.rad(0), math.rad(-80)), .2)
		Torso.Neck.C0 = clerp(Torso.Neck.C0, NeckCF * angles(math.rad(10), math.rad(0), math.rad(20)), .2)
		RW.C0 = clerp(RW.C0, CFrame.new(1.3, 0.5, -.3) * angles(math.rad(0), math.rad(50), math.rad(80)), .2)
		LW.C0 = clerp(LW.C0, CFrame.new(-.5, 0.5, -.5) * angles(math.rad(0), math.rad(-150), math.rad(-90)), .2)
		RH.C0 = clerp(RH.C0, cn(1, -1, 0) * RHCF * angles(math.rad(-5), math.rad(30), math.rad(0)), .2)
		LH.C0 = clerp(LH.C0, cn(-1, -1, 0) * LHCF * angles(math.rad(-5), math.rad(30), math.rad(0)), .2)
		FakeHandleWeld.C0 = clerp(FakeHandleWeld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(30), math.rad(0)), .2)
		PH1Weld.C0 = clerp(PH1Weld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(70), math.rad(0)), .1)
		PH2Weld.C0 = clerp(PH2Weld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(70), math.rad(0)), .1)
	end
	CreateSound("http://roblox.com/asset/?id=320557563", Torso, 1, 0.7)
	CreateSound("http://roblox.com/asset/?id=199149072", Torso, .5, .8)
	CreateSound("http://roblox.com/asset/?id=199149119", Torso, .6, .9)
	for i = 0, 1, 0.04 do
		swait()
		RootJoint.C0 = clerp(RootJoint.C0, RootCF * cn(0, 0, 0) * angles(math.rad(0), math.rad(0), math.rad(80)), .2)
		Torso.Neck.C0 = clerp(Torso.Neck.C0, NeckCF * angles(math.rad(5), math.rad(-5), math.rad(-80)), .2)
		RW.C0 = clerp(RW.C0, CFrame.new(1.3, 0.5, 0) * angles(math.rad(0), math.rad(20), math.rad(90)), .2)
		LW.C0 = clerp(LW.C0, CFrame.new(-1.5, 0.5, 0) * angles(math.rad(0), math.rad(-20), math.rad(-50)), .2)
		RH.C0 = clerp(RH.C0, cn(1, -1, 0) * RHCF * angles(math.rad(-5), math.rad(-30), math.rad(0)), .1)
		LH.C0 = clerp(LH.C0, cn(-1, -1, 0) * LHCF * angles(math.rad(-5), math.rad(-30), math.rad(0)), .1)
		FakeHandleWeld.C0 = clerp(FakeHandleWeld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(-90), math.rad(0)), .2)
		PH1Weld.C0 = clerp(PH1Weld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(0), math.rad(0)), .6)
		PH2Weld.C0 = clerp(PH2Weld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(0), math.rad(0)), .6)
	end
	attack = false
	Character.Humanoid.WalkSpeed = 10
	con1:disconnect()
	con2:disconnect()
end

function SLAY()
	attack = true
	Character.Humanoid.WalkSpeed = 5
	while true do
		swait()
		if charge == true then
			RootJoint.C0 = clerp(RootJoint.C0, RootCF * cn(0, 0, -.2) * angles(math.rad(0), math.rad(0), math.rad(40)), .3)
			Torso.Neck.C0 = clerp(Torso.Neck.C0, NeckCF * angles(math.rad(0), math.rad(0), math.rad(-40)), .3)
			RW.C0 = clerp(RW.C0, CFrame.new(1.5, 0.5, 0) * angles(math.rad(0), math.rad(70), math.rad(130)), .3)
			LW.C0 = clerp(LW.C0, CFrame.new(-1, 0.5, 0) * angles(math.rad(0), math.rad(-30), math.rad(-90)), .3)
			RH.C0 = clerp(RH.C0, cn(1, -.8, -.5) * RHCF * angles(math.rad(-3), math.rad(0), math.rad(10)), .3)
			LH.C0 = clerp(LH.C0, cn(-1, -1, 0) * LHCF * angles(math.rad(-3), math.rad(0), math.rad(40)), .3)
			FakeHandleWeld.C0 = clerp(FakeHandleWeld.C0, cn(.5, 0, 0) * angles(math.rad(0), math.rad(30), math.rad(-30)), .2)
			PHC1weld.C0 = clerp(PHC1weld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(0), math.rad(0)), .3)
			PHC2weld.C0 = clerp(PHC2weld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(0), math.rad(0)), .3)
		elseif charge == false then
			break
		end
	end
	CreateSound("http://roblox.com/asset/?id=360125313", Torso, 1, 1)
	con1 = HitboxA.Touched:connect(function(hit) 
		CreateSound("http://roblox.com/asset/?id=206082273", hit.Parent.Head, 1, 1)
		Damage(hit.Parent, math.random(20, 30), 0.2, BrickColor.new("Really black"), BrickColor.new("Royal purple"), "rbxassetid://199149137" , 1)
		for i = 1, 10 do
			BreakEffect(BrickColor.new("Really red"), hit.Parent:WaitForChild("Head").CFrame, 0.5, math.random(5, 20), 0.5)
		end
		con1:disconnect()
	end) 
	con2 = HitboxB.Touched:connect(function(hit) 
		CreateSound("http://roblox.com/asset/?id=206082273", hit.Parent.Head, 1, 1)
		Damage(hit.Parent, math.random(20, 30), 0.2, BrickColor.new("Really black"), BrickColor.new("Royal purple"), "rbxassetid://199149137" , 1)
		for i = 1, 10 do
			BreakEffect(BrickColor.new("Really red"), hit.Parent:WaitForChild("Head").CFrame, 0.5, math.random(5, 20), 0.5)
		end
		con2:disconnect()
	end) 
	for i = 0, 1, 0.05 do
		swait()
		RootJoint.C0 = clerp(RootJoint.C0, RootCF * cn(0, 0, 0) * angles(math.rad(-20), math.rad(0), math.rad(0)), .2)
		Torso.Neck.C0 = clerp(Torso.Neck.C0, NeckCF * angles(math.rad(10), math.rad(0), math.rad(0)), .2)
		RW.C0 = clerp(RW.C0, CFrame.new(1.2, 1, .7) * angles(math.rad(180), math.rad(0), math.rad(-50)), .2)
		LW.C0 = clerp(LW.C0, CFrame.new(-1.2, 1, .7) * angles(math.rad(180), math.rad(0), math.rad(50)), .2)
		RH.C0 = clerp(RH.C0, cn(1, -1, 0) * RHCF * angles(math.rad(-3), math.rad(0), math.rad(-20)), .2)
		LH.C0 = clerp(LH.C0, cn(-1, -1, 0) * LHCF * angles(math.rad(-3), math.rad(0), math.rad(20)), .2)
		FakeHandleWeld.C0 = clerp(FakeHandleWeld.C0, cn(0, 0, 0) * angles(math.rad(-10), math.rad(10), math.rad(50)), .2)
		PHC1weld.C0 = clerp(PHC1weld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(0), math.rad(0)), .4)
		PHC2weld.C0 = clerp(PHC2weld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(0), math.rad(0)), .4)
	end
	for i = 1, 2 do
		CreateSound("http://roblox.com/asset/?id=320557563", Torso, 1, 0.7)
	end
	for i = 0, 1, 0.03 do
		swait()
		RootJoint.C0 = clerp(RootJoint.C0, RootCF * cn(0, 0, 0) * angles(math.rad(20), math.rad(0), math.rad(0)), .3)
		Torso.Neck.C0 = clerp(Torso.Neck.C0, NeckCF * angles(math.rad(-10), math.rad(0), math.rad(0)), .3)
		RW.C0 = clerp(RW.C0, CFrame.new(1, .5, -.7) * angles(math.rad(40), math.rad(0), math.rad(-50)), .3)
		LW.C0 = clerp(LW.C0, CFrame.new(-1, .5, -.7) * angles(math.rad(40), math.rad(0), math.rad(50)), .3)
		RH.C0 = clerp(RH.C0, cn(1, -1, 0) * RHCF * angles(math.rad(-3), math.rad(0), math.rad(20)), .3)
		LH.C0 = clerp(LH.C0, cn(-1, -1, 0) * LHCF * angles(math.rad(-3), math.rad(0), math.rad(-20)), .3)
		FakeHandleWeld.C0 = clerp(FakeHandleWeld.C0, cn(0, 0, 0) * angles(math.rad(20), math.rad(-20), math.rad(50)), .3)
		PHC1weld.C0 = clerp(PHC1weld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(0), math.rad(0)), .6)
		PHC2weld.C0 = clerp(PHC2weld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(0), math.rad(0)), .6)
	end
	attack = false
	con1:disconnect()
	con2:disconnect()
	Character.Humanoid.WalkSpeed = 10
end

function DIVIDE()
	attack = true
	CreateSound("http://roblox.com/asset/?id=203691492", Torso, 1, 1)
	for i = 0, 1, 0.05 do
		swait()
		Torso.Velocity = RootPart.CFrame.lookVector * 40
		RootJoint.C0 = clerp(RootJoint.C0, RootCF * cn(0, 0, -.2) * angles(math.rad(-10), math.rad(0), math.rad(80)), .2)
		Torso.Neck.C0 = clerp(Torso.Neck.C0, NeckCF * angles(math.rad(5), math.rad(-5), math.rad(-60)), .2)
		RW.C0 = clerp(RW.C0, CFrame.new(1.3, 0.5, -.3) * angles(math.rad(0), math.rad(70), math.rad(50)), .2)
		LW.C0 = clerp(LW.C0, CFrame.new(-1, 0.5, -.5) * angles(math.rad(0), math.rad(-70), math.rad(-70)), .2)
		RH.C0 = clerp(RH.C0, cn(1, -.95, 0) * RHCF * angles(math.rad(7), math.rad(-30), math.rad(0)), .2)
		LH.C0 = clerp(LH.C0, cn(-1, -1, -.2) * LHCF * angles(math.rad(-5), math.rad(-30), math.rad(50)), .2)
		FakeHandleWeld.C0 = clerp(FakeHandleWeld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(0), math.rad(0)), .3)
		PHC1weld.C0 = clerp(PHC1weld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(0), math.rad(0)), .1)
		PHC2weld.C0 = clerp(PHC2weld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(0), math.rad(0)), .1)
	end
	CreateSound("http://roblox.com/asset/?id=360125286", Torso, 1, 1)
	CreateSound("http://roblox.com/asset/?id=320557563", Torso, 1, .8)
	local joj = HitboxA.Touched:connect(function(hit) 
		hit.Parent:WaitForChild('Humanoid').PlatformStand = true
		Ult = true
	end)
	for i = 0, 1, 0.04 do
		swait()
		RootJoint.C0 = clerp(RootJoint.C0, RootCF * cn(0, 0, -.5) * angles(math.rad(20), math.rad(0), math.rad(-80)), .2)
		Torso.Neck.C0 = clerp(Torso.Neck.C0, NeckCF * angles(math.rad(-10), math.rad(-10), math.rad(80)), .2)
		RW.C0 = clerp(RW.C0, CFrame.new(1.5, 0.5, 0) * angles(math.rad(0), math.rad(10), math.rad(80)), .2)
		LW.C0 = clerp(LW.C0, CFrame.new(-1.5, 0.5, 0) * angles(math.rad(0), math.rad(-10), math.rad(-80)), .2)
		RH.C0 = clerp(RH.C0, cn(1, -1, -.3) * RHCF * angles(math.rad(-3), math.rad(40), math.rad(-10)), .2)
		LH.C0 = clerp(LH.C0, cn(-1, -.3, 0) * LHCF * angles(math.rad(-8), math.rad(40), math.rad(-10)), .2)
		FakeHandleWeld.C0 = clerp(FakeHandleWeld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(-50), math.rad(0)), .2)
		PHC1weld.C0 = clerp(PHC1weld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(0), math.rad(0)), .1)
		PHC2weld.C0 = clerp(PHC2weld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(0), math.rad(0)), .1)
	end
	if Ult == true then
		Character.Humanoid.Jump = true
		CreateSound("http://roblox.com/asset/?id=199145327", Torso, 1, .8)
		for i = 0, 1, 0.05 do
			swait()
			Torso.Velocity = Vector3.new(0, 100, 0)
			RootJoint.C0 = clerp(RootJoint.C0, RootCF * cn(0, 0, -.5) * angles(6 * i, math.rad(0), math.rad(0)), .2)
			Torso.Neck.C0 = clerp(Torso.Neck.C0, NeckCF * angles(math.rad(10), math.rad(0), math.rad(0)), .3)
			RW.C0 = clerp(RW.C0, CFrame.new(1.2, 1, .7) * angles(math.rad(180), math.rad(0), math.rad(-50)), .3)
			LW.C0 = clerp(LW.C0, CFrame.new(-1.2, 1, .7) * angles(math.rad(180), math.rad(0), math.rad(50)), .3)
			RH.C0 = clerp(RH.C0, cn(1, -1, 0) * RHCF * angles(math.rad(-3), math.rad(0), math.rad(-20)), .3)
			LH.C0 = clerp(LH.C0, cn(-1, -1, 0) * LHCF * angles(math.rad(-3), math.rad(0), math.rad(20)), .3)
			FakeHandleWeld.C0 = clerp(FakeHandleWeld.C0, cn(0, 0, 0) * angles(math.rad(-10), math.rad(10), math.rad(50)), .3)
			PHC1weld.C0 = clerp(PHC1weld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(0), math.rad(0)), .5)
			PHC2weld.C0 = clerp(PHC2weld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(0), math.rad(0)), .5)
		end
		CreateSound("http://roblox.com/asset/?id=360125313", Torso, 1, 1)
		for i = 0, 1, 0.017 do
			swait()
			RootJoint.C0 = clerp(RootJoint.C0, RootCF * cn(0, 0, -.5) * angles(math.rad(-10), math.rad(0), math.rad(0)), .2)
			Torso.Neck.C0 = clerp(Torso.Neck.C0, NeckCF * angles(math.rad(30), math.rad(0), math.rad(0)), .3)
			RW.C0 = clerp(RW.C0, CFrame.new(1.2, 1, .7) * angles(math.rad(180), math.rad(0), math.rad(-50)), .3)
			LW.C0 = clerp(LW.C0, CFrame.new(-1.2, 1, .7) * angles(math.rad(180), math.rad(0), math.rad(50)), .3)
			RH.C0 = clerp(RH.C0, cn(1, -1, 0) * RHCF * angles(math.rad(-3), math.rad(0), math.rad(-50)), .2)
			LH.C0 = clerp(LH.C0, cn(-1, -1, 0) * LHCF * angles(math.rad(-3), math.rad(0), math.rad(50)), .2)
			FakeHandleWeld.C0 = clerp(FakeHandleWeld.C0, cn(0, 0, 0) * angles(math.rad(-10), math.rad(10), math.rad(50)), .3)
			PHC1weld.C0 = clerp(PHC1weld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(0), math.rad(0)), .6)
			PHC2weld.C0 = clerp(PHC2weld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(0), math.rad(0)), .6)
		end
		CreateSound("http://roblox.com/asset/?id=320557563", Torso, 1, .7)
		con1 = HitboxA.Touched:connect(function(hit) 
			Damage(hit.Parent, math.random(50, 100), 0.2, BrickColor.new("Really black"), BrickColor.new("Royal purple"), "rbxassetid://199149137" , 1)
			CreateSound("http://roblox.com/asset/?id=206082327", hit.Parent.Head, 1, 1)
			CreateSound("http://roblox.com/asset/?id=206082273", hit.Parent.Head, 1, 1)
			for i = 1, 10 do
				BreakEffect(BrickColor.new("Really red"), hit.Parent:WaitForChild("Head").CFrame, 0.5, math.random(5, 20), 0.5)
			end
			con1:disconnect()
		end) 
		con2 = HitboxB.Touched:connect(function(hit) 
			Damage(hit.Parent, math.random(50, 100), 0.2, BrickColor.new("Really black"), BrickColor.new("Royal purple"), "rbxassetid://199149137" , 1)
			CreateSound("http://roblox.com/asset/?id=206082327", hit.Parent.Head, 1, 1)
			CreateSound("http://roblox.com/asset/?id=206082273", hit.Parent.Head, 1, 1)
			for i = 1, 10 do
				BreakEffect(BrickColor.new("Really red"), hit.Parent:WaitForChild("Head").CFrame, 0.5, math.random(5, 20), 0.5)
			end
			con2:disconnect()
		end) 
		for i = 0, 1, 0.05 do
			swait()
			RootJoint.C0 = clerp(RootJoint.C0, RootCF * cn(0, 0, 0) * angles(math.rad(40), math.rad(0), math.rad(0)), .3)
			Torso.Neck.C0 = clerp(Torso.Neck.C0, NeckCF * angles(math.rad(10), math.rad(0), math.rad(0)), .3)
			RW.C0 = clerp(RW.C0, CFrame.new(1, .5, -.7) * angles(math.rad(40), math.rad(0), math.rad(-50)), .3)
			LW.C0 = clerp(LW.C0, CFrame.new(-1, .5, -.7) * angles(math.rad(40), math.rad(0), math.rad(50)), .3)
			RH.C0 = clerp(RH.C0, cn(1, -1, 0) * RHCF * angles(math.rad(-3), math.rad(0), math.rad(20)), .3)
			LH.C0 = clerp(LH.C0, cn(-1, -1, 0) * LHCF * angles(math.rad(-3), math.rad(0), math.rad(-20)), .3)
			FakeHandleWeld.C0 = clerp(FakeHandleWeld.C0, cn(0, 0, 0) * angles(math.rad(20), math.rad(-20), math.rad(50)), .3)
			PHC1weld.C0 = clerp(PHC1weld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(0), math.rad(0)), .6)
			PHC2weld.C0 = clerp(PHC2weld.C0, cn(0, 0, 0) * angles(math.rad(0), math.rad(0), math.rad(0)), .6)
		end
		con1:disconnect()
		con2:disconnect()
	end
	Ult = false
	attack = false
	joj:disconnect()
end

function MakeGrenade()
	local m = Instance.new("Model", Character)
	m.Name = "GrenadeModel"
	GrenadeHandle = CreatePart(m, Enum.Material.SmoothPlastic, 0, 0, "Royal purple", "Handle", Vector3.new(0.720000029, 0.720000029, 0.720000029))
	GrenadeHandleWeld = CreateWeld(m, Character["Left Arm"], GrenadeHandle, CFrame.new(0, 0, 0, 1, 0, 0, 0, 1, 0, 0, 0, 1), CFrame.new(0.0399894714, 0.0880355835, -1.03497469, -1, -1.29144695e-010, -1.65926451e-007, 1.65926451e-007, -5.96046377e-008, -1, 1.291348e-010, -1, 5.96046377e-008))
	GrenadeHandleWeld.Name = "HandleWeld"
	CreateMesh("SpecialMesh", GrenadeHandle, Enum.MeshType.Sphere, "", Vector3.new(0, 0, 0), Vector3.new(1, 1, 1))
	Part = CreatePart(m, Enum.Material.SmoothPlastic, 0, 0, "Really black", "Part", Vector3.new(0.24000001, 0.24000001, 0.24000001))
	Partweld = CreateWeld(m, GrenadeHandle, Part, CFrame.new(0, 0, 0, 1, 0, 0, 0, 1, 0, 0, 0, 1), CFrame.new(1.52587891e-005, 6.07967377e-006, -0.305992126, -0.99999994, 2.91125325e-005, 0.000388413115, 0.0003884123, -2.87742059e-005, 1, 2.91237066e-005, 1, 2.87628955e-005))
	CreateMesh("SpecialMesh", Part, Enum.MeshType.FileMesh, "http://www.roblox.com/asset/?id=18430887", Vector3.new(0, 0, 0), Vector3.new(0.18720001, 0.18720001, 0.766799986))
	Button = CreatePart(m, Enum.Material.SmoothPlastic, 0, 0, "Royal purple", "Button", Vector3.new(0.287999928, 0.24000001, 0.251999915))
	Buttonweld = CreateWeld(m, GrenadeHandle, Button, CFrame.new(0, 0, 0, 1, 0, 0, 0, 1, 0, 0, 0, 1), CFrame.new(0, -0.378005981, -2.2649765e-006, 1, 0, 4.87636868e-018, 0, 1, 0, 4.87636868e-018, 0, 1))
	CreateMesh("CylinderMesh", Button, "", "", Vector3.new(0, 0, 0), Vector3.new(1, 0.150000006, 1))
	Part = CreatePart(m, Enum.Material.SmoothPlastic, 0, 0, "Royal purple", "Part", Vector3.new(0.287999928, 0.24000001, 0.287999928))
	Partweld = CreateWeld(m, GrenadeHandle, Part, CFrame.new(0, 0, 0, 1, 0, 0, 0, 1, 0, 0, 0, 1), CFrame.new(-3.81469727e-006, -0.323997498, 4.05311584e-006, 1, 0, 4.87636868e-018, 0, 1, 0, 4.87636868e-018, 0, 1))
	CreateMesh("CylinderMesh", Part, "", "", Vector3.new(0, 0, 0), Vector3.new(1, 0.299999982, 1))
	Part = CreatePart(m, Enum.Material.Neon, 0, 0, "Really black", "Part", Vector3.new(0.24000001, 0.24000001, 0.24000001))
	Partweld = CreateWeld(m, GrenadeHandle, Part, CFrame.new(0, 0, 0, 1, 0, 0, 0, 1, 0, 0, 0, 1), CFrame.new(1.52587891e-005, 9.77516174e-006, -0.395996094, -0.99999994, 2.91125325e-005, 0.000388413115, 0.0003884123, -2.87742059e-005, 1, 2.91237066e-005, 1, 2.87628955e-005))
	CreateMesh("SpecialMesh", Part, Enum.MeshType.FileMesh, "http://www.roblox.com/asset/?id=18430887", Vector3.new(0, 0, 0), Vector3.new(0.0468000025, 0.0468000025, 0.259199947))
	Part = CreatePart(m, Enum.Material.Neon, 0, 0, "Really black", "Part", Vector3.new(0.24000001, 0.24000001, 0.24000001))
	Partweld = CreateWeld(m, GrenadeHandle, Part, CFrame.new(0, 0, 0, 1, 0, 0, 0, 1, 0, 0, 0, 1), CFrame.new(0, -0.396003723, 5.96046448e-007, 1, 0, 4.87636868e-018, 0, 1, 0, 4.87636868e-018, 0, 1))
	CreateMesh("SpecialMesh", Part, Enum.MeshType.Sphere, "", Vector3.new(0, 0, 0), Vector3.new(0.300000012, 0.300000012, 0.300000012))

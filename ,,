        a3.Speed = NumberRange.new(0)
        a3.VelocitySpread = 55
        a3.EmissionDirection = "Top"
local p1 = Instance.new("Part",workspace)
p1.CanCollide = false
p1.Anchored = true
p1.BrickColor = BrickColor.new("Really black")

mouse.KeyDown:connect(function(key)
 if key == "f" then
                if attack == false then
                        attack = true
                        Chat("AH,SO YOU WANT TO DIE?") --20329976
        local p1 = Instance.new("Part",workspace)
        p1.CanCollide = false
        p1.BrickColor = BrickColor.new("Really black")
